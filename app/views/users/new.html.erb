<%= form_for @user do |f| %>
<fieldset>
  <legend>Sign Up</legend>
    <div>
      <%= f.label :username %>
      <%= f.text_field :username %>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <%= f.label :password %>
      <%= f.password_field :password_digest %>
      <%= f.submit 'Sign up' %>
    </div>
</fieldset>
<% end %>
